{
  "entrypoint": "src/components/sdl-srch-bar.js",
  "shell": "src/components/sdl-srch-bar.js",
  "fragments": [],
  "sources": [
    "demo/data/*",
    "demo/index.html.build-component",
    "tools/*",
    "node_modules/@webcomponents/*",
    "src/components/sdl-srch-bar.js",
    "src/components/sdl-srch-bar.js"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "node_modules/@webcomponents/**/*",
    "node_modules/@polymer/iron-demo-helpers/**/*"
  ],
  "builds": [
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "minify": true,
        "compile": "es2015",
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": false,
      "preset": "es6-bundled"
    }
  ],
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}