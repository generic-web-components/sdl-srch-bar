{
  "entrypoint": "demo/index.html",
  "shell": "demo/index.html",
  "fragments": [
    "src/components/sdl-srch-bar.js"
  ],
  "sources": [
    "src/components/sdl-srch-bar.js",
    "demo/data/*",
    "demo/index.html",
    "tools/*",
    "node_modules/@webcomponents/*",
    "demo/index.html",
    "demo/index.html",
    "src/components/sdl-srch-bar.js"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "node_modules/@webcomponents/**/*",
    "node_modules/@polymer/iron-demo-helpers/**/*"
  ],
  "builds": [
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "minify": true,
        "compile": "es2015",
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": false
      },
      "bundle": true,
      "addServiceWorker": false,
      "addPushManifest": false,
      "preset": "es6-bundled"
    },
    {
      "name": "es5-bundled",
      "js": {
        "minify": true,
        "compile": "es5",
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": false
      },
      "bundle": true,
      "addServiceWorker": false,
      "addPushManifest": false,
      "preset": "es5-bundled"
    }
  ],
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}